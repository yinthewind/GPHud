{
  "name": "GPHud",
  "version": "1.0",
  "description": "Global Poker Hud",
  "permissions": [
    "activeTab",
    "storage",
	"downloads",
    "tabs"
  ],
  "background": {
  	"scripts": ["background.js"],
	"persistent": false
  },
  "content_scripts": [{
    "matches": ["https://play.globalpoker.com/*"],
    "run_at": "document_idle",
    "js": [
      "inject.js"
    ]
  }],
  "browser_action": {},
  "manifest_version": 2
}
